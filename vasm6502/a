F00:0168       vdp_initialize_pattern_table:
F00:0169         pha
               S02:00001D63:  48
F00:0170         phx
               S02:00001D64:  DA
F00:0171         vdp_write_vram VDP_PATTERN_TABLE_BASE
F04:0001         pha
               S02:00001D65:  48
F04:0002         lda #<(VDP_PATTERN_TABLE_BASE)
               S02:00001D66:  A9 00
F04:0003         sta VDP_REG
               S02:00001D68:  8D 01 88
F04:0004         lda #(VDP_WRITE_VRAM_BIT | >VDP_PATTERN_TABLE_BASE)
               S02:00001D6B:  A9 40
F04:0005         sta VDP_REG
               S02:00001D6D:  8D 01 88
F04:0006         pla
               S02:00001D70:  68
F00:0172         lda #<vdp_pattern                         ; load the start address of the patterns to zero page
               S02:00001D71:  A9 00
F00:0173         sta VDP_PATTERN_INIT
               S02:00001D73:  85 30
F00:0174         lda #>vdp_pattern
               S02:00001D75:  A9 37
F00:0175         sta VDP_PATTERN_INIT_HI
               S02:00001D77:  85 31
F00:0176       vdp_pattern_table_loop:
F00:0177         lda (VDP_PATTERN_INIT)                  ; load A with the value at VDP_PATTERN_INIT 
               S02:00001D79:  B2 30
F00:0178         sta VDP_VRAM                            ; and store it to VRAM
               S02:00001D7B:  8D 00 88
F00:0179         inc VDP_PATTERN_INIT
               S02:00001D7E:  E6 30
F00:0180         lda VDP_PATTERN_INIT
               S02:00001D80:  A5 30
F00:0181         bne wopeee
               S02:00001D82:  D0 02
F00:0182         inc VDP_PATTERN_INIT_HI
               S02:00001D84:  E6 31
F00:0183       wopeee:
F00:0184         lda VDP_PATTERN_INIT
               S02:00001D86:  A5 30
F00:0185         cmp #<vdp_pattern_end
               S02:00001D88:  C9 00
F00:0186         bne vdp_pattern_table_loop
               S02:00001D8A:  D0 ED
F00:0187         lda VDP_PATTERN_INIT_HI
               S02:00001D8C:  A5 31
F00:0188         cmp #>vdp_pattern_end
               S02:00001D8E:  C9 4F
F00:0189         bne vdp_pattern_table_loop
               S02:00001D90:  D0 E7
F00:0190         plx
               S02:00001D92:  FA
F00:0191         pla
               S02:00001D93:  68
F00:0192         rts
               S02:00001D94:  60
