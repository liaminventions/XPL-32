F00:0007       ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
F00:0008       
F00:0009       ; TMS9918A
F00:0010       VDP_VRAM               = $8800		; address to set MODE low for a video ram operation on the TMS9918A Video Display Proces
F00:0011       VDP_REG                = $8801		; address to set MODE high for a video register operation on the TMS9918A Video Display 
F00:0012       VDP_WRITE_VRAM_BIT     = %01000000  	; pattern of second vram address write: 01AAAAAA
F00:0013       VDP_REGISTER_BITS      = %10000000  	; pattern of second register write: 10000RRR
F00:0014       
F00:0015       VDP_PATTERN_TABLE_BASE = $0000
F00:0016       VDP_SPRITE_PATTERN_TABLE_BASE = $1800
F00:0017       VDP_COLOR_TABLE_BASE = $2000
F00:0018       VDP_NAME_TABLE_BASE = $3800
F00:0019       VDP_SPR_ATT_TABLE_BASE = $3B00
F00:0020       
F00:0021       TEXT_LOC		= $00FF
F00:0022       
F00:0023       ; zero page addresses
F00:0024       VDP_PATTERN_INIT    	= $30
F00:0025       VDP_PATTERN_INIT_HI 	= $31
F00:0026       
F00:0027       VDP_NAME_POINTER        = $32
F00:0028       
F00:0029         .org $0f00
F00:0030         .macro vdp_write_vram			; macro to store address in vdp_reg for write
F00:0037       
F00:0038       ;;;;;;;;;;;;;;;;;;; reset ;;;;;;;;;;;;;;;;;;;
F00:0039       
F00:0040       reset:
F00:0041       
F00:0042       ;;;;;;;;;;;;;;;;;;; setup subroutines ;;;;;;;;;;;;;;;;;;;;;;;
F00:0043       
F00:0044         sei
               S01:00000F00:  78
F00:0045         stz $b00e     ; argguahububefhia! that darn via! short circut!!1!! this is d fix
               S01:00000F01:  9C 0E B0
F00:0046       
F00:0047         jsr vdp_set_registers
               S01:00000F04:  20 45 0F
F00:0048         jsr vdp_setup
               S01:00000F07:  20 38 0F
F00:0049         lda #$ff	; activate display
               S01:00000F0A:  A9 FF
F00:0050         sta $b002
               S01:00000F0C:  8D 02 B0
F00:0051         sta $b000
               S01:00000F0F:  8D 00 B0
F00:0052         lda #0
               S01:00000F12:  A9 00
F00:0053         jsr InitSid
               S01:00000F14:  20 00 10
F00:0054         ;cli
F00:0055       holding:
F00:0056         ;;jsr changecolor
F00:0057         lda VDP_REG
               S01:00000F17:  AD 01 88
F00:0058         and #$80
               S01:00000F1A:  29 80
F00:0059         beq holding
               S01:00000F1C:  F0 F9
F00:0060         lda #$0e
               S01:00000F1E:  A9 0E
F00:0061         sta VDP_REG
               S01:00000F20:  8D 01 88
F00:0062         lda #$87
               S01:00000F23:  A9 87
F00:0063         sta VDP_REG
               S01:00000F25:  8D 01 88
F00:0064         jsr PlaySid
               S01:00000F28:  20 03 10
F00:0065         lda #$00
               S01:00000F2B:  A9 00
F00:0066         sta VDP_REG
               S01:00000F2D:  8D 01 88
F00:0067         lda #$87
               S01:00000F30:  A9 87
F00:0068         sta VDP_REG
               S01:00000F32:  8D 01 88
F00:0069         jmp holding
               S01:00000F35:  4C 17 0F
F00:0070       
F00:0071       ;;;;;;;;;;;;;;;;;;; vdp_setup subroutines ;;;;;;;;;;;;;;;;;;;;
F00:0072       
F00:0073       vdp_setup:
F00:0074         jsr vdp_initialize_name_table
               S01:00000F38:  20 7C 0F
F00:0075         jsr vdp_initialize_color_table
               S01:00000F3B:  20 95 1D
F00:0076         ;jsr vdp_write_name_table
F00:0077         jsr vdp_initialize_pattern_table
               S01:00000F3E:  20 63 1D
F00:0078         jsr vdp_enable_display
               S01:00000F41:  20 C7 1D
F00:0079         rts
               S01:00000F44:  60
F00:0080       
F00:0081       ;;;;;;;;;;;;;;;;;;; vdp_set_registers ;;;;;;;;;;;;;;;;;;;;;;;;
F00:0082       
F00:0083       vdp_set_registers:
F00:0084         pha
               S01:00000F45:  48
F00:0085         phx
               S01:00000F46:  DA
F00:0086         ldx #0
               S01:00000F47:  A2 00
F00:0087       .loop:
F00:0088         lda vdp_register_inits,x
               S01:00000F49:  BD D4 1D
F00:0089         sta VDP_REG
               S01:00000F4C:  8D 01 88
F00:0090         txa
               S01:00000F4F:  8A
F00:0091         ora #VDP_REGISTER_BITS 				; combine the register number with the second write pattern
               S01:00000F50:  09 80
F00:0092         sta VDP_REG
               S01:00000F52:  8D 01 88
F00:0093         inx
               S01:00000F55:  E8
F00:0094         cpx #8
               S01:00000F56:  E0 08
F00:0095         bne .loop
               S01:00000F58:  D0 EF
F00:0096         plx
               S01:00000F5A:  FA
F00:0097         pla
               S01:00000F5B:  68
F00:0098         rts
               S01:00000F5C:  60
F00:0099       
F00:0100       wait:
F00:0101       	phx
               S01:00000F5D:  DA
F00:0102       	phy
               S01:00000F5E:  5A
F00:0103               tay          ; load secondary loop cycle count (A reg)
               S01:00000F5F:  A8
F00:0104               ldx  #$ff
               S01:00000F60:  A2 FF
F00:0105       delay   dex          ; (2 cycles)
               S01:00000F62:  CA
F00:0106               bne  delay   ; (3 cycles in loop, 2 cycles at end)
               S01:00000F63:  D0 FD
F00:0107               dey          ; (2 cycles)
               S01:00000F65:  88
F00:0108               bne  delay   ; (3 cycles in loop, 2 cycles at end)
               S01:00000F66:  D0 FA
F00:0109       	ply
               S01:00000F68:  7A
F00:0110       	plx
               S01:00000F69:  FA
F00:0111       	rts
               S01:00000F6A:  60
F00:0112       
F00:0113       changecolor:
F00:0114         pha
               S01:00000F6B:  48
F00:0115         inc vdp_register_7
               S01:00000F6C:  EE DB 1D
F00:0116         lda vdp_register_7
               S01:00000F6F:  AD DB 1D
F00:0117         sta VDP_REG
               S01:00000F72:  8D 01 88
F00:0118         lda #$87
               S01:00000F75:  A9 87
F00:0119         sta VDP_REG
               S01:00000F77:  8D 01 88
F00:0120         pla
               S01:00000F7A:  68
F00:0121         rts
               S01:00000F7B:  60
F00:0122       
F00:0123       ;;;;;;;;;;;;;;;;;;; vdp_initialize_name_table ;;;;;;;;;;;;;;;;;;;
F00:0124       
F00:0125       vdp_initialize_name_table:
F00:0126         pha
               S01:00000F7C:  48
F00:0127         phx
               S01:00000F7D:  DA
F00:0128         phy
               S01:00000F7E:  5A
F00:0129         vdp_write_vram VDP_NAME_TABLE_BASE
F01:0001         pha
               S01:00000F7F:  48
F01:0002         lda #<(VDP_NAME_TABLE_BASE)
               S01:00000F80:  A9 00
F01:0003         sta VDP_REG
               S01:00000F82:  8D 01 88
F01:0004         lda #(VDP_WRITE_VRAM_BIT | >VDP_NAME_TABLE_BASE)
               S01:00000F85:  A9 78
F01:0005         sta VDP_REG
               S01:00000F87:  8D 01 88
F01:0006         pla
               S01:00000F8A:  68
F00:0130         stz VDP_NAME_POINTER
               S01:00000F8B:  64 32
F00:0131         ;lda #$20
F00:0132         ldy #0
               S01:00000F8D:  A0 00
F00:0133       vdp_name_table_loop:
F00:0134         sty VDP_VRAM
               S01:00000F8F:  8C 00 88
F00:0135         iny
               S01:00000F92:  C8
F00:0136         bne vdp_name_table_loop
               S01:00000F93:  D0 FA
F00:0137       
F00:0138         inc VDP_NAME_POINTER
               S01:00000F95:  E6 32
F00:0139         ldx VDP_NAME_POINTER
               S01:00000F97:  A6 32
F00:0140         cpx #3
               S01:00000F99:  E0 03
F00:0141         bne vdp_name_table_loop
               S01:00000F9B:  D0 F2
F00:0142         
F00:0143         ply
               S01:00000F9D:  7A
F00:0144         plx
               S01:00000F9E:  FA
F00:0145         pla
               S01:00000F9F:  68
F00:0146         rts
               S01:00000FA0:  60
F00:0147       
F00:0148       vdp_write_name_table:
F00:0149         pha
               S01:00000FA1:  48
F00:0150         phx
               S01:00000FA2:  DA
F00:0151         vdp_write_vram TEXT_LOC
F02:0001         pha
               S01:00000FA3:  48
F02:0002         lda #<(TEXT_LOC)
               S01:00000FA4:  A9 FF
F02:0003         sta VDP_REG
               S01:00000FA6:  8D 01 88
F02:0004         lda #(VDP_WRITE_VRAM_BIT | >TEXT_LOC)
               S01:00000FA9:  A9 40
F02:0005         sta VDP_REG
               S01:00000FAB:  8D 01 88
F02:0006         pla
               S01:00000FAE:  68
F00:0152         ldx #0
               S01:00000FAF:  A2 00
F00:0153       .loop:
F00:0154         lda text_vdp,x
               S01:00000FB1:  BD D4 1D
F00:0155         beq end_write
               S01:00000FB4:  F0 07
F00:0156         sta VDP_VRAM
               S01:00000FB6:  8D 00 88
F00:0157         inx
               S01:00000FB9:  E8
F00:0158         jmp .loop
               S01:00000FBA:  4C B1 0F
F00:0159       end_write:
F00:0160         plx
               S01:00000FBD:  FA
F00:0161         pla
               S01:00000FBE:  68
F00:0162         rts 
               S01:00000FBF:  60
F00:0163       
