

     5 PRINT CHR$(12)
     10 FOR X=0 TO 255
     20 IF X < 32 THEN GOTO 80
     30 IF X < 127 THEN GOTO 120
     40 IF X = 127 THEN PRINT CHR$(26) ; CHR$(127)
     50 IF X > 127 THEN GOTO 120
     80 PRINT CHR$(26) ; CHR$(X);
     85 RESTORE
     90 GOTO 140
     120 PRINT CHR$(X);
     130 RESTORE
     140 FOR Y=1 TO 15
     150 READ D
     155 DATA 16,32,48,64,80,96,112,128,144,160,176,192,208,224,240
     160 IF X=D THEN PRINT CHR$(13) : NEXT X
     170 NEXT Y
     175 RESTORE
     180 NEXT X
