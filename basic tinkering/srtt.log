

    5 PRINTCHR$(12)
    10 PRINT "SRTT"
    20 PRINT "RAM MANAGER"
    30 PRINT "WOULD YOU LIKE TO CONTINUE? (Y/N)"
    40 INPUT A$
    50 IF A$ = "Y" THEN GOTO 100
    60 IF A$ = "N" THEN GOTO 200
    70 PRINTCHR$(12):GOTO30
    100 PRINT "EFF/ECO":INPUT A$
     110 IF A$ = "ECO" THEN GOTO 150
    120 IF A$ = "EFF" THEN GOTO 300
    130 PRINTCHR$(12):GOTO100
    150 PRINT"OPERATION COMPLETE."
    160 FOR D1=0 TO 5
    170 FOR D2=0 TO 255
    180 NEXT D2
    190 NEXT D1
    195 GOTO 400
    200 PRINT"ARE YOU SURE":INPUT A$
    210 IF A$="Y" THEN END
    220 IF A$="N" THEN PRINTCHR$(12):GOTO5
    230 PRINTCHR$(12):GOTO200
    300 PRINTCHR$(12);"CLEARING RAM..."
    305 FOR O=1 TO 10
    310 FOR A = 512 TO 1024
    320 D = PEEK(A)
    330 POKE A+(1024*O),D
    340 NEXT A
    400 PRINT CHR$(12); "YOU HAVE BEEN SRTTIFIED."
    410 END

